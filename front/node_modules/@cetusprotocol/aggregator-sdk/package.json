{
  "name": "@cetusprotocol/aggregator-sdk",
  "version": "1.2.0",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "scripts": {
    "build": "tsup --format cjs,esm --dts",
    "dev": "tsup --watch",
    "test": "node -r esm node_modules/.bin/jest",
    "test:gas": "bun test tests/benchmarks/gas-benchmark.ts",
    "benchmark": "bun tests/benchmarks/run-benchmark.ts",
    "benchmark:quick": "bun tests/benchmarks/run-benchmark.ts --max-tests 10",
    "benchmark:cetus": "bun tests/benchmarks/run-benchmark.ts --providers CETUS",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "publish:test": "node version.mjs && npm publish --tag experimental"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@mysten/sui": "^1.0.5",
    "@pythnetwork/pyth-sui-js": "^2.1.0",
    "bip39": "^3.1.0",
    "dotenv": "^16.4.5",
    "json-bigint": "^1.0.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-typescript": "^7.24.1",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.3",
    "typescript": "^5.0.0",
    "@types/bn.js": "^5.1.5",
    "@types/bun": "latest",
    "@types/json-bigint": "^1.0.4",
    "bn.js": "^5.2.1",
    "decimal.js": "^10.4.3",
    "tsup": "^8.0.2"
  },
  "overrides": {
    "chalk": "5.3.0",
    "strip-ansi": "7.1.0",
    "color-convert": "2.0.1",
    "color-name": "1.1.4",
    "is-core-module": "2.13.1",
    "error-ex": "1.3.2",
    "has-ansi": "5.0.1"
  }
}